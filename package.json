{
  "name": "peer-file",
  "version": "0.0.4",
  "description": "A little library for doing p2p file transfers over webrtc.",
  "main": "index.js",
  "scripts": {
    "test": "tape test/*.js"
  },
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/latest",
      "chrome/latest",
      "firefox/latest",
      "safari/latest",
      "opera/latest"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:michaelrhodes/peer-file.git"
  },
  "keywords": [
    "webrtc",
    "peerjs",
    "datachannel",
    "airdrop",
    "file",
    "transfer",
    "progress",
    "pause",
    "resume"
  ],
  "author": "Michael Rhodes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michaelrhodes/peer-file/issues"
  },
  "homepage": "https://github.com/michaelrhodes/peer-file",
  "dependencies": {
    "brfs": "^1.1.2",
    "emitter-component": "^1.1.1",
    "filereader-stream": "git://github.com/michaelrhodes/filereader-stream",
    "portable-uuid": "^0.1.0"
  },
  "devDependencies": {
    "tape": "^3.0.0"
  }
}
