{
  "name": "peer-file",
  "version": "0.0.3",
  "description": "A little library for doing p2p file transfers over webrtc.",
  "main": "index.js",
  "scripts": {
    "test": "tape test/*.js"
  },
  "browserify": {
    "transform": ["brfs"]
  },
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/latest",
      "chrome/latest",
      "firefox/latest",
      "safari/latest",
      "opera/latest"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:michaelrhodes/peer-file.git"
  },
  "keywords": [
    "webrtc",
    "peerjs",
    "datachannel",
    "airdrop",
    "file",
    "transfer",
    "progress",
    "pause",
    "resume"
  ],
  "author": "Michael Rhodes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michaelrhodes/peer-file/issues"
  },
  "homepage": "https://github.com/michaelrhodes/peer-file",
  "dependencies": {
    "filereader-stream": "^0.1.1",
    "inherits": "^2.0.1",
    "uuid": "^1.4.1"
  },
  "devDependencies": {
    "brfs": "^1.1.2",
    "tape": "^2.13.3"
  }
}
